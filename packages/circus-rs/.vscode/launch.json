  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch CIRCUS RS Server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/server.js"
    },
    {
      "name": "CIRCUS RS Nodemon",
      "type": "node",
      "request": "attach",
      "processId": "${command:PickProcess}",
      "restart": true,
      "protocol": "inspector"
    }
  ]
}